<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="multicolorbutton" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <item name="colorButtonNormal">@android:color/holo_blue_dark</item>
    </style>

    <style name="simpleButton" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">@android:color/holo_blue_dark</item>
    </style>

</resources>
